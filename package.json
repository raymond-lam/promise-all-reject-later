{
  "name": "promise-all-reject-later",
  "version": "1.0.1",
  "description": "A drop-in replacement for Promise.all() which does not fail-fast.",
  "main": "index.js",
  "keywords": [
    "Promise.all",
    "Promise.all()",
    "promise",
    "all",
    "promise all",
    "fail-fast",
    "fail fast",
    "no-fail-fast",
    "no fail fast",
    "reject",
    "last",
    "late",
    "reject last",
    "reject late",
    "return last",
    "return late"
  ],
  "scripts": {
    "clean": "rimraf coverage",
    "test": "eslint --ignore-path=.gitignore . && cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha"
  },
  "author": {
    "name": "Raymond Lam",
    "email": "ray@lam-ray.com"
  },
  "homepage": "https://github.com/raymond-lam/promise-all-reject-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/raymond-lam/promise-all-reject-later.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-istanbul": "^5.1.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "greenkeeper-lockfile": "^1.15.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "temp-directory": "./coverage/.nyc_output"
  }
}
